{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceRoot}/backEnd",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run", "dev"
            ],
        },
        {
            "name": "parking_system",
            "request": "launch",
            "type": "dart",
            "args": [
                "--no-sound-null-safety"
            ]
        },
        {
            "name": "parking_system (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--no-sound-null-safety"
            ]
        },
        {
            "name": "parking_system (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--no-sound-null-safety"
            ]
        }
    ]
}